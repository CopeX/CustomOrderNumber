<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Magenuts_CustomOrderNumber::ordernumber" title="Magenuts Custom Order Number Configuration"/>
                        </resource>
                        <resource id="Magenuts_CustomOrderNumber::reset" title="Magenuts Custom Order Number Reset Sequence">
                            <resource id="Magenuts_CustomOrderNumber::resetcreditmemo" title="Magenuts Custom Order Number Reset Creditmemo"/>
                            <resource id="Magenuts_CustomOrderNumber::resetinvoice" title="Magenuts Custom Order Number Reset Invoice"/>
                            <resource id="Magenuts_CustomOrderNumber::resetorder" title="Magenuts Custom Order Number Reset Order"/>
                            <resource id="Magenuts_CustomOrderNumber::resetshipment" title="Magenuts Custom Order Number Reset Shipment"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
